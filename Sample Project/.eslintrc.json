{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "amd": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "indent": ["off", "tab"],
    "linebreak-style": ["off", "unix"],
    "quotes": ["warn", "double"],
    "semi": ["off", "never"],
    "camelcase": ["error", { "properties": "always" }]
  }
}
